  <div class="row">
  <div class="col s2"></div>
  <div class ="col s8 left-align">
    {{#each articles}}
      <div class="card-panel">
        <h4>{{this.title}}</h4>
        <a href = "{{this.link}}" class="waves-effect waves-light btn light-blue lighten-1" id="btnSignUp">Read Article</a>
        <form action="/deleteArticle/{{this._id}}" method="POST">
          <input class="btn add-note-button brown lighten-1" type="submit" value="Delete">
        </form>
      </div>
    {{/each}}
  </div>
</div>